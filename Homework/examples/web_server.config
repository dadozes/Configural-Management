// Конфигурация веб-сервера - пример 1
define default_port 8080
define max_clients 1000

(
    server_name: q(My Web Server);
    port: $default_port$;
    max_connections: $max_clients$;
    ssl_enabled: true;
    
    database: (
        host: q(localhost);
        port: 5432;
        name: q(myapp);
        credentials: (
            username: q(admin);
            password: q(secret123);
        );
    );
    
    logging: (
        level: q(INFO);
        file: q(/var/log/server.log);
        max_size: 10485760;
    );
)
